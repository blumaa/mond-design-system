{
  "name": "@mond-design-system/component-lib",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "main": "index.ts",
  "types": "index.ts",
  "scripts": {
    "build": "yarn build:tokens && yarn build:components",
    "build:tokens": "tsx tokens/scripts/build.ts",
    "build:components": "tsc",
    "dev": "concurrently \"yarn dev:tokens\" \"yarn dev:components\" \"yarn storybook\"",
    "dev:tokens": "nodemon --watch tokens/tokens -e json --exec 'yarn build:tokens'",
    "dev:components": "tsc --watch",
    "test": "jest",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.6.7",
    "@storybook/addon-interactions": "^8.6.7",
    "@storybook/addon-links": "^8.6.7",
    "@storybook/addon-themes": "^8.6.12",
    "@storybook/blocks": "^8.6.7",
    "@storybook/react": "^8.6.7",
    "@storybook/react-vite": "^8.6.7",
    "@storybook/test": "^8.6.7",
    "@testing-library/jest-dom": "^5.0.0",
    "@testing-library/react": "^13.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.5.0",
    "concurrently": "^8.2.2",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "nodemon": "^3.1.9",
    "storybook": "^8.6.7",
    "terser": "^5.40.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.8"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    }
  }
}